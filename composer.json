{
    "name": "devcbh/laravel-ai-provider",
    "description": "A Laravel AI wrapper package with multiple provider drivers.",
    "type": "library",
    "version": "2.1.0",
    "autoload": {
        "psr-4": {
            "Devcbh\\LaravelAiProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devcbh\\LaravelAiProvider\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devcbh\\LaravelAiProvider\\AiServiceProvider"
            ],
            "aliases": {
                "Ai": "Devcbh\\LaravelAiProvider\\Facades\\Ai"
            }
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Villegas",
            "email": "dev.cbh@gmail.com"
        }
    ]
}
